					<div
						class="cropPicture h-64 w-auto bg-[url('/images/people/chaney.jpg')] bg-cover bg-center"
					>
					</div>
					<div
						class="cropPictureReverse col-span-1 flex h-auto w-auto flex-col border-2 border-white bg-cover bg-center px-5 py-5"
					>
						<h1 class="font-hype text-xl leading-none pl-3">CHANEY</h1>
						<h1 class="font-hype text-2xl leading-none">HOLLIS</h1>
						<span class="border-b py-1 leading-none">
							Outreach Coordinator, Programming Member</span
						>
